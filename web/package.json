{
  "name": "supertonic-browser-extension",
  "version": "1.0.0",
  "description": "Provides Supertonic text-to-speech voices as a browser extension",
  "scripts": {
    "build-debug": "webpack --mode=development",
    "build-release": "webpack --mode=production",
    "prod-sync": "aws s3 sync --delete --metadata-directive REPLACE --cache-control max-age=86400,public build/release s3://lsdsoftware-websites/supertonic.ttstool.com",
    "deploy-prod": "npm run build-release && npm run prod-sync"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ken107/piper-browser-extension.git"
  },
  "author": "Hai Phan <hai.phan@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ken107/piper-browser-extension/issues"
  },
  "homepage": "https://github.com/ken107/piper-browser-extension/tree/supertonic#readme",
  "devDependencies": {
    "@types/chrome": "^0.1.32",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "copy-webpack-plugin": "^13.0.1",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "webpack": "^5.90.2",
    "webpack-bundle-analyzer": "^5.0.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@lsdsoftware/message-dispatcher": "^1.0.6",
    "onnxruntime-web": "1.23.2",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "rxjs": "^7.8.2",
    "use-immer": "^0.11.0"
  }
}
